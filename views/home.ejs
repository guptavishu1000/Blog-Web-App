<%- include( "partials/header") -%>
<h1>Home</h1>
<%= s %>
<% posts.forEach(function(x){ %>
<div>
    <h3> <%= x.title %> </h3>
    <p> <%= x.content.substring(0, 100)+"..."%>
        <% if(x.content.length > 100){ %>
         <a href="posts/<%= x.title %>">read more</a> 
         <% } %>
        </p>
</div>
<%})%>
